this.CDN_details = function(idCDN)
{

$.ajax({
        url: "../py/form.py/cdn_details",
	data: {cdnID:idCDN},
	type: 'POST',
        dataType: "json",
        cache: false,
        success: function(response){
				     if(response.success == false) {alert("Logging and password are incorrect!");} //window.location='../login.htm';}
				     else {
					   machine_name = response.machine_names;
					   machine_public_ip = response.machine_public_ip;
					   machine_creation_date = response.machine_creation_date;
					   started = response.started;
					   machine_flavor = response.machine_flavor;
                                           machine_type = response.machine_type;
					   var i = 0;
  					   var create='';

					   create +='<table  class="table table-striped">'
					   create +='<tr > <td ><label>Machine Name:</label></td><td><label>Type</label> </td><td><label>Flavor</label> </td><td><label>Public IP</label> </td><td ><label>Creation Date:</label> </td><td><label>Status</label> </td></tr>'
					   while(i<machine_name.length) {
 						if (started[i]==1) {show='Active'}
						else {show='Inactive'}
 						create +='<tr > <td ><label>' + machine_name[i] + '</label></td><td><label>' + machine_flavor[i] + '</label> </td><td><label>' + machine_type[i] + '</label> </td><td><label>' + machine_public_ip[i] + '</label> </td><td ><label>' + machine_creation_date[i] + '</label> </td><td><label>' + show + '</label> </td></tr>'
					   	i = i + 1	
					    }

				            create +='</table>'
				            //create +='<div><button class="btn btn-lg btn-primary btn-block" type="button"><a onclick="../htm/listcdn.htm">Back</a></button></div>'
 					    $("#cdn_zone").empty();	
                                            $("#cdn_zone").append(create);	

					
				       }	
 				     
 	                           }
						
});//end ajax 					


}


////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////




this.CDN_modify_page = function(idCDN)
{

$.ajax({
        url: "../py/form.py/cdn_modify_details",
	data: {cdnID:idCDN},
	type: 'POST',
        dataType: "json",
        cache: false,
        success: function(response){
				     if(response.success == false) {alert("Some Issue Occured");} //window.location='../login.htm';}
				     else {
					   cdn_name = response.cdn_name;
					   vm_machine_names = response.vm_machine_names;
					   var i = 0;
  					   var create='';
					   sessionStorage.setItem('number_of_vms', vm_machine_names.length);
					   sessionStorage.setItem('cdn_id', idCDN);
					   create +='<table  class="table table-striped">'
					   create +="<tr > <td ><label>CDN Name : </label></td><td><input type='text' id='cdnname' class='form-control' value='" + cdn_name + "' /></td></tr>"
					   while(i<vm_machine_names.length) {
						j=i;
						j=j+1;
 						create +="<tr><td><label>Virtual Machine " + j + " Name : </label></td><td><input type='text' id='vm_machine" + i + "' class='form-control' value='" + vm_machine_names[i] + "' > </td></tr>";
					   	i = i + 1
					    }

				            create +='</table>'
					    create += "<div class='form-group'><button class='btn btn-lg btn-primary btn-block' data-disable type='button' onclick='cdn.modify_details_checkvalue();'>Modify</button>"
 					    $("#cdn_zone").empty();	
                                            $("#cdn_zone").append(create);	

					
				       }	
 				     
 	                           }
						
});//end ajax 					


}






