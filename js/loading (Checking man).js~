this.deleterowFromdetail = function(row, nameMachine)
	{
	  $(row).parent().parent().parent().remove();
	 // alert(Object.keys(myFlavors));
  	 delete myFlavors[nameMachine]; 
	 delete myTypes[nameMachine];
	 delete myRegions[nameMachine];
	// alert(Object.keys(myFlavors));
	}



