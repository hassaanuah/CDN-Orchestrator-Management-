this.deleteCDN = function(idCDN) {
$.ajax({
        url: "../py/form.py/deleteCDN",
	data: {cdnID:idCDN},
	type: 'POST',
        dataType: "json",
        cache: false,
        success: function(response){ if (response.success==true) {cdn.loadCDNs();}
 	                           }
						
});//end ajax 					

//cdn.loadCDNs();

} 
//"this.deleteCDN('+idCDN[i]+');"



